  "bugs": "https://github.com/bangle-io/bangle-io/issues",
  "devDependencies": {
    "@bangle.io/plop-configs": "workspace:*",
    "@types/react": "^18.3.11",
    "@biomejs/biome": "1.9.3",
    "@swc/jest": "^0.2.36",
    "@types/bun": "^1.1.11",
    "@types/jest": "^29.5.13",
    "jest": "^29.7.0",
    "plop": "^4.0.1",
    "typescript": "^5.6.3"
  },
  "homepage": "https://bangle.io",
  "license": "AGPL-3.0-or-later",
  "pnpm": {
    "overrides": {}
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "quoteProps": "consistent",
    "arrowParens": "always",
    "printWidth": 80
  },
  "private": true,
  "scripts": {
    "custom-validation": "bun packages/tooling/custom-scripts/scripts-validate/all.ts",
    "lint:ci": "pnpm run typecheck && biome ci .",
    "lint:fix": "biome check . --fix",
    "typecheck": "tsc -b",
    "lint": "biome check .",
    "start": "pnpm -r --filter \"browser-entry\" run dev",
    "build": "pnpm run precursor && pnpm -r --filter \"browser-entry\" run build",
    "test:ci": "pnpm jest --ci",
    "test": "pnpm jest",
    "precursor": "echo \"ok\""
  },
  "module": "index.ts",
  "type": "module",
  "peerDependencies": {},
  "packageManager": "pnpm@9.12.1+sha512.e5a7e52a4183a02d5931057f7a0dbff9d5e9ce3161e33fa68ae392125b79282a8a8a470a51dfc8a0ed86221442eb2fb57019b0990ed24fab519bf0e1bc5ccfc4"
