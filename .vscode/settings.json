{
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#65f6f2",
    "activityBar.background": "#65f6f2",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#f04af4",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#65f6f2",
    "statusBar.background": "#35f3ee",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#0ee7e1",
    "statusBarItem.remoteBackground": "#35f3ee",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#35f3ee",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#35f3ee99",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#35f3ee",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "cSpell.words": [
    "abortable",
    "backlink",
    "capp",
    "Comlink",
    "comms",
    "dhancha",
    "editorbar",
    "naukar",
    "smallscreen",
    "titlebar",
    "Unmount",
    "vite",
    "worktree",
    "Worktrees"
  ],
  "javascript.preferences.importModuleSpecifier": "project-relative",
  "git.postCommitCommand": "sync",
  "search.exclude": {
    "**/node_modules/*": true
  },
  "editor.codeActionsOnSave": {
    "quickfix.biome": "explicit",
    "source.organizeImports.biome": "explicit"
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  }
}
